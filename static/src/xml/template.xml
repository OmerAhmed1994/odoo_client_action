<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
	<t t-name="odoo_client_action.DashboardAction" owl="1">
		<div class="header">
			<div class="left">
				
			</div>
			<div class="right">
			</div>
		</div>
		<div class="main">
			<div class="row">
				<div t-foreach="state.users" t-as="user" t-key="user.id" class="col-lg-4 col-md-6 col-sm-12 col-xs-12" >
					<div class="card" >
						<div class="card-body header">
							<div>
								<div t-esc="user.name"/>
							</div>
							<div>
								<div t-esc="user.id"/>
							</div>
						</div>
						<ul class="list-group list-group-flush">
							<li class="list-group-item">
								item 1
							</li>
							<li class="list-group-item">
								item 2
							</li>
						</ul>
						<div class="card-body footer">
							<button t-on-click="button1" type="button" class="btn btn-primery background-red" >button 1 </button>

							<button t-on-click="button2" type="button" class="btn btn-primery" >button</button>
						</div>
					</div>			
				</div>	
			</div>				
		</div>
	</t>
</templates>